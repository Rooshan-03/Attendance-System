{
  "expo": {
    "name": "Attendly",
    "slug": "my-expo-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/AppIcon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/AppIcon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.rooshan.attendly"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/AppIcon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.rooshan.attendly"
    },
    "owner": "shani_03",
    "extra": {
      "eas": {
        "projectId": "e5524f68-7b0a-4c89-a1f6-86b17b4bc745"
      }
    },
    "plugins": [
      "expo-font"
    ],
    "experiments": {
      "tsconfigPaths": true
    }
  }
}